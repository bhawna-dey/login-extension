{
    "name": "Oauth2 test",
    "description": "",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "./logo16.png",
        "32": "./logo32.png",
        "48": "./logo48.png",
        "128": "./logo128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "identity",
        "scripting"
    ],
    "background":{
        "service_worker":"./background.js"
    },
    "content_security_policy": {
        "extension_pages":"script-src 'self'; object-src 'self';"
    }
}